	});
	
	
    });
</script>
<script> 
    const sb = (event) => {
        event.preventDefault()
        var check = $("input:radio[name=rating]:checked").val()
        var btn = document.getElementById("btt")
        btn.innerHTML = "loading..."
        btn.disabled = true
        axios.post("/send-review", { 
            review: $("#t_input").val(),
            rating: parseInt(check),
            id: '<%= part.id %>'
        }).then(res => {
            if(res.data.message){
                $.alert(res.data.message)
                location.reload()
                btn.innerHTML = "Continue"
                btn.disabled = false
            }else if(res.data.error) {
                $.alert(res.data.error)
                btn.innerHTML = "Continue"
                btn.disabled = false
            }
        }).catch(err => {
            if(err){
                if(err.response){
                    console.log(err.response)
                 $.alert(err.response.data.error)
                btn.innerHTML = "Continue"
                btn.disabled = false                 
                }else {
                    $.alert("There was an error processing your request")
                btn.innerHTML = "Continue"
                btn.disabled = false                    
                }
            }
        })
    }
